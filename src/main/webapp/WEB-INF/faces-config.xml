<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <application>
        <resource-bundle>
            <base-name>/Activity</base-name>
            <var>activity</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Config</base-name>
            <var>config</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Params</base-name>
            <var>params</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Controller</base-name>
            <var>controller</var>
        </resource-bundle>
    </application>
</faces-config>
